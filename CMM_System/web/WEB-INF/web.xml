<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>LoginServletStudent</servlet-name>
        <servlet-class>loginStudent.LoginServletStudent</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginDaoStudent</servlet-name>
        <servlet-class>loginStudent.LoginDaoStudent</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>main.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginDaoStaff</servlet-name>
        <servlet-class>loginStaff.LoginDaoStaff</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServletStaff</servlet-name>
        <servlet-class>loginStaff.LoginServletStaff</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>stud_programServlet</servlet-name>
        <servlet-class>programStudent.stud_programServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>staff_programServlet</servlet-name>
        <servlet-class>programStaff.staff_programServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProfileStaff</servlet-name>
        <servlet-class>profile.ProfileStaff</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProfileStudent</servlet-name>
        <servlet-class>profile.ProfileStudent</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditProfileStudent</servlet-name>
        <servlet-class>profile.EditProfileStudent</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditProfileStaff</servlet-name>
        <servlet-class>profile.EditProfileStaff</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServletStudent</servlet-name>
        <url-pattern>/LoginServletStudent</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginDaoStudent</servlet-name>
        <url-pattern>/LoginDaoStudent</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginDaoStaff</servlet-name>
        <url-pattern>/LoginDaoStaff</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServletStaff</servlet-name>
        <url-pattern>/LoginServletStaff</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>stud_programServlet</servlet-name>
        <url-pattern>/stud_programServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>staff_programServlet</servlet-name>
        <url-pattern>/staff_programServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProfileStaff</servlet-name>
        <url-pattern>/ProfileStaff</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProfileStudent</servlet-name>
        <url-pattern>/ProfileStudent</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditProfileStudent</servlet-name>
        <url-pattern>/EditProfileStudent</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditProfileStaff</servlet-name>
        <url-pattern>/EditProfileStaff</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>login.jsp</welcome-file>
    </welcome-file-list>
</web-app>
